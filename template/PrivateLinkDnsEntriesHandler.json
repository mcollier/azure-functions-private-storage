{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "privateDnsZoneName": {
            "type": "string"
        },
        "Ipconfigs": {
            "type": "array",
            "metadata": {
                "description": "resource id to ipConfigurations array of private endpoint nic"
            }
        },
        "dnsEntriesTemplateUri": {
            "type": "string",
            "metadata": {
                "description": "Uri for template that creates private dns entries"
            }
        },
        "deploymentNameSuffix": {
            "type": "string"
        }
    },
    "variables": {
    },
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "name": "[concat('PrivateDnsFQDN', copyIndex(1), '-', uniqueString(resourceGroup().id, deployment().name), '-', parameters('deploymentNameSuffix'))]",
            "properties": {
                "mode": "Incremental",
                "parameters": {
                    "privateDnsZoneName": {
                        "value": "[parameters('privateDnsZoneName')]"
                    },
                    "storageNicIpConf": {
                        "value": "[parameters('Ipconfigs')[copyIndex()]]"
                    }
                },
                "templateLink": {
                    "uri": "[parameters('dnsEntriesTemplateUri')]"
                }
            },
            "copy": {
                "name": "ipconfigCopy",
                "count": "[length(parameters('Ipconfigs'))]",
                "mode": "parallel"
            }
        }
    ],
    "outputs": {
    },
    "functions": [
    ]
}